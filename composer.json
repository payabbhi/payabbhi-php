{
    "name": "payabbhi/payabbhi-php",
    "description": "Payabbhi PHP package provides client library for Payabbhi API's",
    "keywords": [
      "payabbhi",
      "payment processing",
      "api"
    ],
    "homepage": "https://payabbhi.com/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Payabbhi Team",
            "homepage": "http://github.com/payabbhi"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0",
      "php-coveralls/php-coveralls": "^2.1"
    },
    "config": {
       "bin-dir": "bin"
   },
    "autoload": {
        "psr-0": {
            "Payabbhi": "src/"
        }
    }
}
